{
  "name": "Electron-Chat",
  "author": "David Rosenblum",
  "version": "0.1.0",
  "description": "Test project for using Electron + React + WebSockets + Node.js + Child_Process",
  "dependencies": {
    "bootstrap": "^4.2.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.3",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "react-scripts build",
    "react": "react-scripts start",
    "dist": "build"
  },
  "homepage": "./",
  "build": {
    "appId": "com.david.electronchat",
    "win": {
      "target": "nsis"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
